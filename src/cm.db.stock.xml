,Stock_Move.QC_Type,Stock_Move.QC_Flag,Stock_Move.product_actual_qty,Stock_Move.product_occupy_qty,Stock_Move.product_enable_qty   
FROM Stock_Move
LEFT JOIN product_product ON Stock_Move.Code=product_product.Code]]>
